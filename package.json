{
  "name": "babel-codemod",
  "version": "0.0.0-development",
  "description": "babel-codemod rewrites JavaScript using babel plugins.",
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "yarn test",
    "lint": "tslint --project .",
    "lint-fix": "tslint --project . --fix",
    "pretest": "yarn lint && tsc --project .",
    "test": "mocha \"test/**/*Test.js\"",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "postmerge": "yarnhook",
    "postcheckout": "yarnhook",
    "postrewrite": "yarnhook"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "bin": {
    "codemod": "./bin/codemod"
  },
  "files": [
    "bin",
    "src/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.js.map"
  ],
  "author": "Brian Donovan <donovan@squareup.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/babel-traverse": "^6.25.3",
    "@types/babylon": "^6.16.2",
    "@types/get-port": "^4.0.0",
    "@types/get-stream": "^3.0.1",
    "@types/glob": "^7.1.0",
    "@types/got": "^8.3.1",
    "@types/make-dir": "^1.0.3",
    "@types/mocha": "^5.2.1",
    "@types/mz": "0.0.32",
    "@types/node": "^10.3.0",
    "@types/prettier": "^1.12.4",
    "@types/rimraf": "2.0.2",
    "@types/semver": "^5.5.0",
    "@types/source-map-support": "^0.4.1",
    "@types/tmp": "^0.0.33",
    "commitlint": "^7.0.0",
    "get-port": "^4.0.0",
    "husky": "^1.0.0",
    "lint-staged": "^8.0.5",
    "make-dir": "^1.3.0",
    "mocha": "^5.2.0",
    "prettier-check": "^2.0.0",
    "rimraf": "2.6.2",
    "semver": "^5.5.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.1",
    "yarnhook": "^0.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/generator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/traverse": "^7.0.0",
    "@babel/types": "^7.0.0",
    "get-stream": "^4.0.0",
    "globby": "^8.0.1",
    "got": "^8.3.1",
    "mz": "^2.7.0",
    "pirates": "^4.0.0",
    "prettier": "^1.13.4",
    "recast": "^0.16.1",
    "resolve": "^1.7.1",
    "source-map-support": "^0.5.6",
    "tmp": "^0.0.33",
    "whatwg-url": "^7.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/square/babel-codemod.git"
  }
}
