{
  "name": "@codemod/cli",
  "version": "3.3.0",
  "description": "codemod rewrites JavaScript and TypeScript",
  "repository": "https://github.com/codemod-js/codemod.git",
  "license": "Apache-2.0",
  "author": "Brian Donovan",
  "type": "module",
  "exports": {
    "bun": "./src/index.ts",
    "default": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "codemod": "./bin/codemod"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "is-ci test:coverage test:watch",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@babel/core": "esm",
    "@babel/generator": "esm",
    "@babel/parser": "esm",
    "@babel/traverse": "esm",
    "@babel/types": "esm",
    "@codemod/core": "workspace:^",
    "@codemod/matchers": "workspace:^",
    "@codemod/parser": "workspace:^",
    "@codemod/utils": "workspace:^",
    "commonjs-extension-resolution-loader": "^0.1.0",
    "cross-fetch": "^3.1.5",
    "find-up": "^5.0.0",
    "get-stream": "^5.1.0",
    "globby": "^11.0.0",
    "ignore": "^5.1.9",
    "is-ci-cli": "^2.2.0",
    "pirates": "^4.0.0",
    "recast": "^0.19.0",
    "regenerator-runtime": "^0.13.3",
    "resolve": "^1.22.1",
    "source-map-support": "^0.5.6",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.3",
    "@types/glob": "^7.1.0",
    "@types/got": "^9.6.7",
    "@types/jest": "^29",
    "@types/node": "^20",
    "@types/resolve": "^1.20.6",
    "@types/rimraf": "3.0.0",
    "@types/semver": "^7.1.0",
    "@types/source-map-support": "^0.5.0",
    "@types/tmp": "^0.2.0",
    "commitlint": "^18",
    "dedent": "^1.5",
    "get-port": "^5.0.0",
    "make-dir": "^3.1.0",
    "prettier": "^3",
    "rimraf": "3.0.2",
    "semver": "^7.3.5",
    "tempy": "^1",
    "typescript": "^5.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}