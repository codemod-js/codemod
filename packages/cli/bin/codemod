#!/usr/bin/env node

const run = (() => {
  try {
    return require('../').default
  } catch {
    process.stderr.write('codemod does not seem to be built')
    process.exit(1)
  }
})()

run(process.argv)
  .then((status) => {
    process.exit(status)
  })
  .catch((err) => {
    console.error(err.stack)
    process.exit(-1)
  })
