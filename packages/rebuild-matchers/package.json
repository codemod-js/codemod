{
  "name": "@codemod/rebuild-matchers",
  "version": "0.1.0",
  "private": true,
  "description": "Rebuilds the bulk of `@codemod/matchers` based on the Babel AST.",
  "repository": "https://github.com/codemod-js/codemod",
  "license": "Apache-2.0",
  "author": "Brian Donovan",
  "type": "module",
  "exports": {
    "bun": "./src/rebuild.ts",
    "default": "./dist/index.js"
  },
  "main": "dist/rebuild.js",
  "types": "dist/rebuild.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "is-ci test:coverage test:watch",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@babel/types": "esm",
    "@codemod/utils": "workspace:^",
    "commonjs-extension-resolution-loader": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "esm",
    "@babel/generator": "esm",
    "@babel/traverse": "esm",
    "@codemod/core": "workspace:^",
    "@codemod/parser": "workspace:^",
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.18.3",
    "@types/jest": "^29",
    "@types/node": "^20",
    "dedent": "^1.5",
    "is-ci-cli": "^2.2.0",
    "typescript": "^5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}